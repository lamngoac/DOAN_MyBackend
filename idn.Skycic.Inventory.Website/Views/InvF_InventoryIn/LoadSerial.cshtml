@model List<InvF_InventoryInInstSerial>

@if (Model != null && Model.Count > 0)
{
    var idx = 0;
    foreach (var item in Model)
    {
        <tr class="data-item trdata" idx="@idx" productcode="@item.ProductCode">
            <input type="hidden" name="Lst_InvF_InventoryInInstSerial[@idx].ProductCode" value="@item.ProductCode" />
            <input type="hidden" name="Lst_InvF_InventoryInInstSerial[@idx].ProductCodeRoot" value="@item.ProductCode" />
            <input type="hidden" name="Lst_InvF_InventoryInInstSerial[@idx].SerialNo" value="@item.SerialNo" />
            <input type="hidden" name="Lst_InvF_InventoryInInstSerial[@idx].InvCodeInActual" value="@item.InvCodeInActual" />
        </tr>
        idx++;
    }
}

