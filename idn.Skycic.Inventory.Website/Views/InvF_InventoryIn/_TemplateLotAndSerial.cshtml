            <input type="hidden" name="Lst_InvF_InventoryInDtlTemp[==idx==].ProductCode" value="==ProductCode==" />
            <input type="hidden" name="Lst_InvF_InventoryInDtlTemp[==idx==].InvCodeInActual" value="==InvCodeInActual==" />
