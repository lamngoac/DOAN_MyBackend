                    @RenderItemFormat("BTN_INVENTORYOUT_DELETE", " <a class=\"btn btn-nc-button\" onclick=\"Delete()\"><i class=\"fas fa-trash\" aria-hidden=\"true\"></i> Xóa</a>")

                }
