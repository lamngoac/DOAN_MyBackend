                {
                    @*@RenderItemFormat("BTN_DETAIL_INVENTORYOUT_EXPORTCROSS", "<a href=\"" + Url.Action("ExportCross", "InvF_InventoryOut", new { IF_InvOutNo = Model.Lst_InvF_InventoryOut[0].IF_InvOutNo }) + "\" class=\"btn btn-nc-button\"><i class=\"far fa-share-square\" aria-hidden=\"true\"></i> Xuất chéo..</a>", "", "")*@
