@{
    var lst_Mst_TempPrintType = new List<Mst_TempPrintType>();
    if (ViewBag.Lst_Mst_TempPrintType != null)
    {
        lst_Mst_TempPrintType = ViewBag.Lst_Mst_TempPrintType as List<Mst_TempPrintType>;
    }
}


<div class="mt-3">
    <div class="col-12">
        @using (Html.BeginForm("Create", "InvFTempPrint", FormMethod.Post, new { id = "manageForm", @class = "form-horizontal" }))
        {
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true)

            <div class="col-12 row">
                <div class="col-12 row">                    
                    <aside class="col-md-4">
                        <div class="form-group margin-bottom-9">
                            <label class="control-label col-md-4" for="InvCodeOut">
                                @("Loại mẫu in".HtmlItemString("TempPrintType_InvFTempPrint"))
                                <span style="color: red "> *</span>
                            </label>
                            <div class="col-md-8">
                                <span class="block input-icon input-icon-right">
                                    <select id="InvCodeAudit" name="InvCodeAudit" class="col-md-12 mstinventory">
                                        <optgroup label="">
                                            <option value="">--Chọn loại mẫu in--</option>
                                            @if (lst_Mst_TempPrintType != null && lst_Mst_TempPrintType.Count > 0)
                                            {
                                                foreach (var item in lst_Mst_TempPrintType)
                                                {
                                                    var TempPrintType = CUtils.StrValue(item.TempPrintType);
                                                    var TempPrintName = CUtils.StrValue(item.TempPrintName);
                                                    <option TempPrintType="@TempPrintType" TempPrintName="@TempPrintName" value="@TempPrintType">@TempPrintName</option>
                                                }
                                            }
                                        </optgroup>                                        
                                    </select>
                                </span>
                            </div>
                        </div>
                    </aside>                    
                </div>
                <div class="col-12 row">
                    <aside class="col-md-8">
                        <div class="form-group" style="float: left; margin-top: 30px; margin-bottom: 10px;">
                            @RenderItemFormat("", "<a href=\"javascript:void(0);\" class=\"btn btn-nc-button\" id=\"btnSave\" onclick=\"SaveData('" + Url.Action("Save", "InvFInvAudit") + "', '0');\"><i class=\"fa fa-plus-circle\" aria-hidden=\"true\"></i> Lưu</a>", "", "")                            
                            @*<a class="btn btn-nc-button" data-toggle="dropdown" aria-expanded="false" style="font-size: 13px; position: relative;"><i class="fas fa-ellipsis-h"></i></a>
                                <ul class="dropdown-menu dropdown-action" role="menu" style="">
                                    <li style="padding: 5px">
                                        <a class="row no-margin col-12 no-padding" onclick="ExportTemplate('@Url.Action("ExportTemplateSerial", "ModalCommon")')" href="javascript:void(0);" style="line-height: 13px; color: #000000"><i class="fas fa-download" aria-hidden="true"></i> Xuất Excel Serial</a>
                                    </li>
                                    <li style="padding: 5px">
                                        <a class="row no-margin col-12 no-padding" onclick="ExportTemplate('@Url.Action("ExportTemplateLo", "ModalCommon")')" data-toggle="modal" href="javascript:void(0);" style="line-height: 13px; color: #000000"><i class="fas fa-download" aria-hidden="true"></i> Nhập Excel lô</a>
                                    </li>
                                    <li style="padding: 5px">
                                        <a class="row no-margin col-12 no-padding" onclick="ExportTemplate('@Url.Action("ExportTemplateQR", "ModalCommon")')" href="javascript:void(0);" style="line-height: 13px; color: #000000"><i class="fas fa-download" aria-hidden="true"></i> Nhập Excel xác thực</a>
                                    </li>
                                </ul>*@
                        </div>
                    </aside>
                </div>
                <div class="col-12 row">
                   
                </div>
            </div>
        }
    </div>
</div>
