    <script type="text/javascript">
        $(document).ready(function () {
            if (commonUtils.checkElementExists('#Profile')) {
                $('#Profile').unbind('click').bind('click', function () {
                    $('#modal_ChangePassword').modal({
                        backdrop: false,
                        keyboard: true
                    });
                });
            }
        });

        function ClosePopupModelChangePassword() {
            if (commonUtils.checkElementExists('#passold')) {
                $('#passold').val('');
            }
            if (commonUtils.checkElementExists('#passnew')) {
                $('#passnew').val('');
            }
            if (commonUtils.checkElementExists('#confpass')) {
                $('#confpass').val('');
            }
            $('#modal_ChangePassword').modal("hide");
            //$('#modal_ChangePassword').html('');
        }

    </script>
