                                        </li>
                                        <li class="nav-item">
                                            @RenderSubMenuItem("MENU_QL_XAC_THUC_TT_SAN_PHAM", "Xác thực thông tin sản phẩm", "Index", "Inv_InventoryBalanceSerial")
                                        </li>
                                    </ul>
                                </li>
                            }
                        }
