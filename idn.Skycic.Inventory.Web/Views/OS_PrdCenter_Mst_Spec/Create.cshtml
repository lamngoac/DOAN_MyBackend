@section Scripts{
    <script src="@Url.Content("~/Scripts/mine/OS_PrdCenter_Mst_Spec.js")"></script>
    <script type="text/javascript">
        @*Active Menu*@
        setAdminCurrentUrl('@Url.Action("Index", "OS_PrdCenter_Mst_Spec")');
        processAjaxForm($('#manageForm'), function (response) {
            processAjaxFormResult(response);
        });

        @* Set focus *@
        $(document).ready(function () {
            $('#BrandCode').focus();
        });

        @* Scroll *@
        $(document).ready(function () {
            $('.scrollable-horizontal').each(function () {
                var $this = $(this);
                $(this).ace_scroll(
                    {
                        horizontal: true,
                        size: $this.attr('data-size') || 6500,
                        mouseWheelLock: true
                    }
                ).css({ 'padding-top': 0 });
            });
        });

        var maxImageWidth = 800,
        totalFiles = 0,
        currentTotalFiles = 0,
        fileTypeError = false,
        fileSizeError = false,
        ListImages = [],
        ListFiles = [],
        ImgId = 0,
        FileId = 0;

        document.getElementById("fileImage").addEventListener('change', handleImageFileSelect, false);
        
        $(document).ready(function () {
            $("#selectedImage").bind('click', function () {
                $('#fileImage').click();
            });

            $("#selectedFile").bind('click', function () {
                $('#fileInput').click();
            });
        });
        
        var objOS_PrdCenter_Mst_Spec = new OS_PrdCenter_Mst_Spec();

        function RemoveImages(thiz) {
            objOS_PrdCenter_Mst_Spec.removeImages(thiz);
        }

        function SelectedImages(thiz) {
            objOS_PrdCenter_Mst_Spec.selectedImages(thiz);
        }

        function SetPrimaryImage(thiz) {
            objOS_PrdCenter_Mst_Spec.setPrimaryImages(thiz);
        }

        function handleImageFileSelect(event) {
            objOS_PrdCenter_Mst_Spec.handleImageFileSelect(event);
        }

        function renderImageFileInfo(file, id) {
            objOS_PrdCenter_Mst_Spec.renderImageFileInfo(file, id);
        }

        function checkError() {
            objOS_PrdCenter_Mst_Spec.checkError();
        }
        document.getElementById("fileInput").addEventListener('change', handleFileSelect, false);

        function handleFileSelect(event) {
            objOS_PrdCenter_Mst_Spec.handleFileSelect(event);
        }

        function renderFileInfo(file, id) {
            objOS_PrdCenter_Mst_Spec.renderFileInfo(file,id);
        }

        function DeleteFile(thiz) {
            objOS_PrdCenter_Mst_Spec.deleteFile(thiz);
        }
        function SaveData() {
            var ajaxSettings = {};
            ajaxSettings.Type = 'post';
            ajaxSettings.DataType = 'json';
            ajaxSettings.Url = '@Url.Action("Create", "OS_PrdCenter_Mst_Spec")';
            var objOS_PrdCenter_Mst_Spec = new OS_PrdCenter_Mst_Spec();
            objOS_PrdCenter_Mst_Spec.ActionType = 'create';
            objOS_PrdCenter_Mst_Spec.ajaxSettings = ajaxSettings;
            objOS_PrdCenter_Mst_Spec.saveData();
        }
    </script>
}