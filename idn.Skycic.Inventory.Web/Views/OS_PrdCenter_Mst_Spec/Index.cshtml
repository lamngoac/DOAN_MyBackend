@*Import Excel*@
<div class="modal fade" id="ImportExcelModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true"></div>

@section Scripts{
    <script src="@Url.Content("~/Scripts/mine/OS_PrdCenter_Mst_Spec.js")"></script>
    <script type="text/javascript">
        @*Active Menu*@
        setAdminCurrentUrl('@Url.Action("Index", "OS_PrdCenter_Mst_Spec")');
        

        @* Set focus *@
        $(document).ready(function () {
            $('#SpecCode').focus();
        });

        @* Scroll *@
        $(document).ready(function () {
            $('.scrollable-horizontal').each(function () {
                var $this = $(this);
                $(this).ace_scroll(
                    {
                        horizontal: true,
                        //styleClass: 'scroll-top',//show the scrollbars on top(default is bottom)
                        size: $this.attr('data-size') || 6500,
                        mouseWheelLock: true
                    }
                ).css({ 'padding-top': 0 });
            });
        });
        $('.scrollable').each(function () {
            var $this = $(this);
            $(this).ace_scroll({
                //size: 500,
                size: $this.attr('data-size') || 1000,
                mouseWheelLock: true
                //styleClass: 'scroll-left scroll-margin scroll-thin scroll-dark scroll-light no-track scroll-visible'
            });
        });

        @* keypress *@
        $(document).on('keypress', function (e) {
            if (e.which == 13) {
                Search();
            }
        });

        function Search()
        {
            $('#manageForm').attr('action', '@Url.Action("Index", "OS_PrdCenter_Mst_Spec")').submit();
        }

        var objOS_PrdCenter_Mst_Spec = new OS_PrdCenter_Mst_Spec();
        function Delete(speccode, specname, orgID) {
            var objOS_PrdCenter_Mst_Spec_Delete = {
                SpecCode: speccode,
                SpecName: specname,
                OrgID: orgID,
            };

            var ajaxSettings = {};
            ajaxSettings.Type = 'post';
            ajaxSettings.DataType = 'json';
            ajaxSettings.Url = '@Url.Action("Delete", "OS_PrdCenter_Mst_Spec")';

            objOS_PrdCenter_Mst_Spec.Image_Message = "<img id=\"exclamation-circle\" style=\"width: 35px; float: left; margin-right: 10px\" src=\"@Url.Content("~/Images/exclamation-circle-solid.svg")\"> THÔNG BÁO";
            objOS_PrdCenter_Mst_Spec.Confirm_Message = "Đồng ý xóa Spec '" + specname + "' ?";
            objOS_PrdCenter_Mst_Spec.ajaxSettings = ajaxSettings;
            objOS_PrdCenter_Mst_Spec.deleteData(objOS_PrdCenter_Mst_Spec_Delete);
        }

        var objCommonExcel = new CommonExcel();
        var objVariablesInit = {
            Id_FormMain: '',
            Id_FormImportExcel: 'manageFormImportExcel',
            Id_Modal: 'ImportExcelModal',
            Id_FileInput: 'file',
        };

        var ajaxSettings_Excel = {};
        ajaxSettings_Excel.Type = 'post';
        ajaxSettings_Excel.DataType = 'json';

        function ShowPopupImportExcel(check) {
            ajaxSettings_Excel.Url = '@Url.Action("ShowPopupImportExcel", "Admin")';
            ajaxSettings_Excel.Check = check;

            objCommonExcel.ajaxSettingsInit();
            objCommonExcel.ajaxSettings = ajaxSettings_Excel;
            objCommonExcel.variablesInit(objVariablesInit);
            objCommonExcel.showPopupImportExcel();
        }

        function ImportFileExcel() {
            ajaxSettings_Excel.Url = '@Url.Action("Import", "OS_PrdCenter_Mst_Spec")';
            if (ajaxSettings_Excel.Check === false) {
                ajaxSettings_Excel.Url = '@Url.Action("ImportMapping", "OS_PrdCenter_Mst_Spec")';
            }
            objCommonExcel.ajaxSettingsInit();
            objCommonExcel.ajaxSettings = ajaxSettings_Excel;
            objCommonExcel.variablesInit(objVariablesInit);
            objCommonExcel.importFileExcel();
        }

        function ExportTemplate(check) {
            ajaxSettings_Excel.Url = '@Url.Action("ExportTemplate", "OS_PrdCenter_Mst_Spec")';
            if (check==false) {
                ajaxSettings_Excel.Url = '@Url.Action("ExportTemplateMapping", "OS_PrdCenter_Mst_Spec")';
            }

            objCommonExcel.ajaxSettingsInit();
            objCommonExcel.ajaxSettings = ajaxSettings_Excel;
            objCommonExcel.exportExcel();
        }

        function Export() {
            ajaxSettings_Excel.Url = '@Url.Action("Export", "OS_PrdCenter_Mst_Spec")';

            var speccode = commonUtils.returnValueTextOrNull('#speccode');
            var specname = commonUtils.returnValueTextOrNull('#specname');
            var brandcode = commonUtils.returnValueTextOrNull('#brandcode');
            var modelcode = commonUtils.returnValueTextOrNull('#modelcode');
            var spectype1 = commonUtils.returnValueTextOrNull('#spectype1');
            var spectype2 = commonUtils.returnValueTextOrNull('#spectype2');
            var flagactive = commonUtils.returnValueTextOrNull('#flagactive');
            var orgid = commonUtils.returnValueTextOrNull('#orgid');

            var ListSearchInput = [
                { Key: 'speccode', Value: speccode },
                { Key: 'specname', Value: specname },
                { Key: 'brandcode', Value: brandcode },
                { Key: 'modelcode', Value: modelcode },
                { Key: 'spectype1', Value: spectype1 },
                { Key: 'spectype2', Value: spectype2 },
                { Key: 'flagactive', Value: flagactive },
                { Key: 'orgid', Value: orgid },
            ];

            //objOS_PrdCenter_Mst_Spec.ajaxSettings = ajaxSettings_Excel;
            //objOS_PrdCenter_Mst_Spec.exportExcel(ListSearchInput);

            objCommonExcel.ajaxSettingsInit();
            objCommonExcel.ajaxSettings = ajaxSettings_Excel;
            objCommonExcel.exportExcel(ListSearchInput);
        }

    </script>
}
