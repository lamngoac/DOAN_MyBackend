@using System.Reflection
@model PageInfo<Mst_Spec>
@{
    Layout = "~/Views/Shared/_LayoutQuantri.cshtml";
    var title = "DANH MỤC HÀNG HÓA";
    ViewBag.Title = title;
    ViewBag.OverflowX = true;
    var startCount = 0;
    var strStartCount = ViewBag.StartCount as string;
    var datasize = 2000;
    var isparent = ViewBag.IsParent;
    if (isparent)
    {
        datasize += 120;
    }
    if (!CUtils.IsNullOrEmpty(strStartCount))
    {
        startCount = Convert.ToInt32(strStartCount);
    }
    var listOS_PrdCenter_Mst_Model = ViewBag.ListOS_PrdCenter_Mst_Model as List<Mst_Model>;
    if (listOS_PrdCenter_Mst_Model == null || listOS_PrdCenter_Mst_Model.Count == 0)
    {
        listOS_PrdCenter_Mst_Model = new List<Mst_Model>();
    }
    var listOS_PrdCenter_Mst_SpecType1 = ViewBag.ListOS_PrdCenter_Mst_SpecType1 as List<Mst_SpecType1>;
    if (listOS_PrdCenter_Mst_SpecType1 == null || listOS_PrdCenter_Mst_SpecType1.Count == 0)
    {
        listOS_PrdCenter_Mst_SpecType1 = new List<Mst_SpecType1>();
    }
    var listOS_PrdCenter_Mst_SpecType2 = ViewBag.ListOS_PrdCenter_Mst_SpecType2 as List<Mst_SpecType2>;
