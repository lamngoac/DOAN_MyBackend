    @*lan 2019_04-13*@
    <script>
        function escapeHtml(text) {
            return text
                .replace(/&/g, "&amp;")
                .replace(/</g, "&lt;")
                .replace(/>/g, "&gt;")
                .replace(/"/g, "&quot;")
                .replace(/'/g, "&#039;");
        }

        function SelectBackground() {
            $('#fileImageBackGround').click();
        }

        function SelectLogo() {
            $('#fileImage').click();
        }
    </script>

