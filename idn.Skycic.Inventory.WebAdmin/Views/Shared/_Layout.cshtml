    <div class="modal" role="dialog" id="modal_ChangePassword">
        @*@Html.Partial("ChangePassword")*@
    </div>

    <script type="text/javascript">
        $(document).ready(function () {
            if (CheckElementExists('#Profile')) {
                $('#Profile').unbind('click').bind('click', function () {
                    $('#modal_ChangePassword').modal({
                        backdrop: false,
                        keyboard: true
                    });
                });
            }
        });

        function ClosePopupModelChangePassword() {
            if (CheckElementExists('#passold')) {
                $('#passold').val('');
            }
            if (CheckElementExists('#passnew')) {
                $('#passnew').val('');
            }
            if (CheckElementExists('#confpass')) {
                $('#confpass').val('');
            }
            $('#modal_ChangePassword').modal("hide");
            //$('#modal_ChangePassword').html('');
        }

    </script>
